{
  "name": "jest-axe",
  "version": "3.1.1",
  "description": "Custom Jest matcher for aXe for testing accessibility",
  "repository": "nickcolley/jest-axe",
  "main": "index.js",
  "files": [
    "index.js",
    "extend-expect.js"
  ],
  "scripts": {
    "test": "jest"
  },
  "keywords": [
    "jest",
    "matcher",
    "axe",
    "accessibility",
    "a11y"
  ],
  "author": "Nick Colley",
  "license": "MIT",
  "engines": {
    "node": ">= 6.0.0"
  },
  "dependencies": {
    "axe-core": "3.1.2",
    "chalk": "^2.4.2",
    "jest-matcher-utils": "24.0.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.5",
    "@testing-library/vue": "^1.2.0",
    "@vue/server-test-utils": "^1.0.0-beta.29",
    "@vue/test-utils": "^1.0.0-beta.29",
    "async-to-gen": "1.4.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "24.1.0",
    "react": "16.7.0",
    "react-dom": "^16.8.6",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "async-to-gen",
      "^.+\\.vue$": "vue-jest",
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@testing-library/react)/)"
    ]
  }
}
